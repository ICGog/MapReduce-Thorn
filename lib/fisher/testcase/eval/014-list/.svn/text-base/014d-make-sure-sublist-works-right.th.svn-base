{
  fun is_012?(L) {
     ~!@hit() ;// @!~ 4;
     ~!@eq(L, [0,1,2]);
     ~!@eq(L.sub(1,2), [1,2]);
     ~!@eq(L(0), 0);
     ~!@eq(L(-1), 2);
     ~!@eq(L(-3), 0);
  }is_012?
  is_012?([0,1,2]);

  L1 = [0,1,2,3];
  is_012?(L1.sub(0, -2));

  is_012?(L1.sub(-4,2).sub(0,-1));

  L2 = [-1, 0,1,2,3,4,5];
  L2a = L2.sub(1, -1); 
  ~!@eq(L2a, [0,1,2,3,4,5]);
  L2b = L2a.sub(0,2);
  is_012?(L2b);

}

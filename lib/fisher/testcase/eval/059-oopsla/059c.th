{
  fun f(L) = L ~ [x:int && (x != 0)?, $(32 div x)];

  ~!@eq(f([4,8]), true)@!~  ;
  ~!@eq(f([0,0]), false)@!~  ;
  
  {~!@eq([1] ~ [x,!$(x)...], true);}
  {~!@eq([1,2] ~ [x,!$(x)...], true);}
  {~!@eq([1,3,2] ~ [x,!$(x)...], true);}
  {~!@eq([1,4,3,2] ~ [x,!$(x)...], true);}
  {~!@eq([] ~ [x,!$(x)...], false);}
  {~!@eq([1,1] ~ [x,!$(x)...], false);}
  {~!@eq([1,1,1] ~ [x,!$(x)...], false);}
  {~!@eq([1,1,1,1] ~ [x,!$(x)...], false);}
  {~!@eq([1,2,2,1] ~ [x,!$(x)...], false);}
  
}

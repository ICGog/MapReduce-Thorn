{
  class A(x) {
    def `+`(y) = [x,y];
    def `-`(y) = {:x,y:};
  }
  a = A(1);
  ~!@eq(a + 2, [1,2])@!~  ;
  ~!@bad(2+a)@!~  ;
  
  ~!@eq(1.5 + 1, 2.5)@!~  ;
  ~!@eq(1.5 + "ow", "1.5ow")@!~  ;
  ~!@eq(1 + "ow", "1ow")@!~  ;
  
  ~!@eq(2 - 0.5, 1.5)@!~  ;
  ~!@eq(2.5 - 2, 0.5)@!~  ;
  
  ~!@eq(a - 2, {:x:1, y:2:})@!~  ;
  ~!@bad(2 - a)@!~  ;
  ~!@bad(2.5 - a)@!~  ;
  
}

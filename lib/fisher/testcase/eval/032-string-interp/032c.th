  fun zong(s, u, l, c) {
//println("032c: toUpper of " + s);
     ~!@eq(s.toUpper(), u)  ;
//println("032c: toLower of " + s);
     ~!@eq(s.toLower(), l)  ;
//println("032c: capitalize of " + s);
     ~!@eq(s.capitalize(), c)  ;
//println("032c: done of " + s);
  }zong 

  zong("", "", "", "");
  zong("a", "A", "a", "A");
  zong("algae", "ALGAE", "algae", "Algae");
  zong("0", "0", "0", "0");
//println("032c-end");
}
